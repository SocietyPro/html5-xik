{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "exclude": [ "angular.js", "filer.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "../docs/",
        "recurse": true
    }
}